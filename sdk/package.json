{
  "name": "kubelt-sdk",
  "version": "0.1.0",
  "description": "Kubelt SDK",
  "bin": {
    "ddt": "target/cli.js"
  },
  "scripts": {
    "cmd:example": "npx shadow-cljs run build.command/example",
    "lint:init": "mkdir .clj-kondo && clj -M:kondo --parallel --dependencies --copy-configs --lint src/",
    "lint:all": "clj -M:kondo --parallel --lint src/",
    "lint:dev": "clj -M:kondo --parallel --lint src/dev/",
    "lint:main": "clj -M:kondo --parallel --lint src/main/",
    "lint:test": "clj -M:kondo --parallel --lint src/test/",
    "build:cli:develop": "npx shadow-cljs compile cli",
    "build:cli:release": "npx shadow-cljs release cli",
    "build:sdk:node:develop": "npx shadow-cljs compile sdk",
    "build:sdk:node:release": "npx shadow-cljs release sdk",
    "build:sdk:web:develop": "npx shadow-cljs compile web",
    "build:sdk:web:release": "npx shadow-cljs release web",
    "test:sdk:node": "npx shadow-cljs compile node-test",
    "test:sdk:web": "npx shadow-cljs compile web-test",
    "test:sdk:js": "tap test/",
    "test": "npm run test:sdk:node && npm run test:sdk:web"
  },
  "author": "Kubelt, Inc.",
  "license": "UNLICENSED",
  "devDependencies": {
    "shadow-cljs": "2.16.12",
    "tap": "15.1.5"
  },
  "dependencies": {
    "@ipld/car": "3.2.3",
    "@ipld/dag-cbor": "7.0.0",
    "@ipld/dag-json": "8.0.7",
    "@multiformats/blake2": "1.0.8",
    "@multiformats/sha3": "2.0.7",
    "blake3": "2.1.7",
    "event-stream": "4.0.1",
    "ipfs-message-port-client": "0.10.3",
    "ipns": "0.16.0",
    "jsonld": "5.2.0",
    "levelup": "5.1.1",
    "libp2p-crypto": "0.21.0",
    "memdown": "6.1.1",
    "multiformats": "9.5.4",
    "n3": "1.12.2",
    "quadstore": "9.1.0",
    "rdf-parse": "1.9.1"
  }
}
